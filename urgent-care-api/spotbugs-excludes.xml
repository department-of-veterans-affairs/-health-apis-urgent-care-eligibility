<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- Exclusions can be defined here and should include a comment on why the finding can be ignored -->
  <!-- Exception is a type in this case, should not be derived from an Exception -->
  <Match>
    <Class name="gov.va.api.health.urgentcare.service.api.resources.Coverage$Exception"/>
    <Bug pattern="NM_CLASS_NOT_EXCEPTION"/>
  </Match>
</FindBugsFilter>
